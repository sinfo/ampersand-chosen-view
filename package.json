{
  "name": "ampersand-chosen-view",
  "description": "Making long, unwieldy select boxes more friendly on AmpresandJS using Chosen.",
  "version": "0.0.1",
  "author": "Francisco Dias <francisco@baiodias.com>",
  "browserify": {
    "transform": [
      "browserify-shim",
      "ampersand-version",
      "browserify-css"
    ]
  },
  "bugs": {
    "url": "https://github.com/xicombd/ampersand-chosen-view/issues"
  },
  "browser": {
    "jquery": "./vendor/jquery-2.1.3.js",
    "chosen": "./vendor/chosen.jquery.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "chosen": {
      "depends": [
        "jquery"
      ]
    }
  },
  "dependencies": {
    "ampersand-dom": "^1.2.7",
    "ampersand-form-view": "^2.2.0",
    "ampersand-version": "^1.0.0",
    "ampersand-view": "^7.0.0",
    "browserify-shim": "^3.8.2",
    "domify": "^1.3.1",
    "matches-selector": "^1.0.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "ampersand-view-conventions": "^1.1.1",
    "beefy": "^2.0.2",
    "browserify": "~4.1.10",
    "browserify-css": "^0.2.0",
    "function-bind": "~0.1.0",
    "jshint": "~2.5.1",
    "phantomjs": "^1.9.7-15",
    "precommit-hook": "~1.0.2",
    "run-browser": "~1.3.1",
    "tap-spec": "~0.2.0",
    "tape": "~2.13.3",
    "tape-run": "~0.1.1"
  },
  "homepage": "https://github.com/xicombd/ampersand-chosen-view",
  "keywords": [
    "forms",
    "ampersand",
    "browser",
    "chosen",
    "select"
  ],
  "license": "MIT",
  "main": "ampersand-chosen-view.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/sinfo/ampersand-chosen-view"
  },
  "scripts": {
    "test": "browserify test/index.js | tape-run -b phantom | tap-spec",
    "start": "run-browser test/*",
    "demo": "beefy demo.js --live true"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/9..latest",
      "firefox/17..latest",
      "firefox/nightly",
      "chrome/22..latest",
      "chrome/canary",
      "opera/12..latest",
      "opera/next",
      "safari/5.1..latest",
      "ipad/6.0..latest",
      "iphone/6.0..latest",
      "android-browser/4.2..latest"
    ]
  }
}
